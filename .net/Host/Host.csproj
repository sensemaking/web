<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>Sensemaking.Host.Web</AssemblyName>
    <RootNamespace>Sensemaking.Host.Web</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" Version="3.1.3" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Sensemaking.Host" Version="1.0.94" />
    <PackageReference Include="Sensemaking.Http" Version="1.0.94" />
  </ItemGroup>

</Project>
